    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="lama" type="Integer" array="False" size=""/>
            <declare name="tax" type="Integer" array="False" size=""/>
            <output expression="&quot;masukkan lama parkir (jam): &quot;" newline="True"/>
            <input variable="lama"/>
            <if expression="lama &lt;= 2">
                <then>
                    <assign variable="tax" expression="5000"/>
                </then>
                <else>
                    <assign variable="tax" expression="5000+(lama-2)*2000"/>
                </else>
            </if>
            <if expression="tax&gt;20000">
                <then>
                    <assign variable="tax" expression="20000"/>
                </then>
                <else/>
            </if>
            <output expression="&quot;fanum tax ppn bea cukai parkir&quot;" newline="True"/>
            <output expression="tax" newline="True"/>
        </body>
    </function>
